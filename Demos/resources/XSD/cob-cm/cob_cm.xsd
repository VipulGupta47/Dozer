<?xml version="1.0" encoding="UTF-8"?>

<xs:schema id="CobCm" xmlns="www.genpact.com" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="www.genpact.com"
	xmlns:common="www.genpact.com/common" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" 
	jaxb:extensionBindingPrefixes="xjc" jaxb:version="1.0" version="0.2">

	<xs:annotation>
		<xs:documentation>A set of procedures, policies, processes and risk based controls that are designed to enable an organization to have a reasonable belief that it knows the true
			identity of each of its clients. This will also include designing an approach to monitoring client activities and transactions that capture the risks associated with their clients and which will 
			satisfactorily identify those transactions or activities that may be unusual or suspicious.
		</xs:documentation>
	</xs:annotation>

	<xs:import schemaLocation="DataTypes/CommonDataTypes.xsd" namespace="www.genpact.com/common" />

	<!-- JAXB extension to add serializable to all classes -->
	<xs:annotation>
		<xs:appinfo>
			<jaxb:globalBindings generateIsSetMethod="false">
				<xjc:serializable uid="1" />
			</jaxb:globalBindings>
		</xs:appinfo>
	</xs:annotation>

	<xs:element name="CobCmData">
		<xs:annotation>
			<xs:documentation>
				Root element of the proprietary XML that acts as a standard format for transferring profile data from the sender's system to COS CM application for profile management.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RequestInfo" minOccurs="1" type="RequestInfo" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>
							Information regarding the request.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EntityProfile" minOccurs="1" type="EntityProfile" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>
							The profile data stored in COB CM application.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ResponseInfo" minOccurs="0" type="ResponseInfo" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>
							Information regarding the response.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="RequestInfo">
		<xs:sequence>
			<xs:element name="RequestDate" minOccurs="1" maxOccurs="1" type="xs:date">
				<xs:annotation>
					<xs:documentation>The date when the request was sent</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SenderIdentification" minOccurs="1" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation>Sender's identification</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReferenceId" minOccurs="1" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation>Sender's reference Id for the request</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EntityType" minOccurs="1" maxOccurs="1" type="common:EntityType">
				<xs:annotation>
					<xs:documentation>Entity type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MessageType" minOccurs="1" maxOccurs="1" type="common:MessageType">
				<xs:annotation>
					<xs:documentation>Request type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DueDate" minOccurs="0" maxOccurs="1" type="xs:date">
				<xs:annotation>
					<xs:documentation>Optional date by when the profile should be completed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChallengeDetails" minOccurs="0" maxOccurs="1" type="ChallengeDetails">
				<xs:annotation>
					<xs:documentation>Details on the challenge request</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentList" minOccurs="0" maxOccurs="unbounded" type="Document">
				<xs:annotation>
					<xs:documentation>List of documents identified for the profile</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="EntityProfile">
		<xs:sequence>
			<xs:element name="Jurisdiction" maxOccurs="1" minOccurs="1">
				<xs:annotation>
					<xs:documentation>The jurisdiction for which the CM is to be done</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BookingLocation" type="xs:string" maxOccurs="1" minOccurs="1"></xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:choice minOccurs="1" maxOccurs="1">
				<xs:element name="Institution" type="Institution" minOccurs="0" maxOccurs="1"></xs:element>
				<xs:element name="Individual" type="Individual" minOccurs="0" maxOccurs="1"></xs:element>
				<xs:element name="Trust" type="Trust" minOccurs="0" maxOccurs="1"></xs:element>
			</xs:choice>
			<xs:element name="ReferenceDocuments" minOccurs="0" maxOccurs="1" type="common:ReferenceDocuments">
				<xs:annotation>
					<xs:documentation>The list of reference documents that were previously identified for this profile.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="LastUpdateDate" type="xs:dateTime" />
		<xs:attribute name="ProfileId" type="xs:string" />
	</xs:complexType>

	<xs:complexType name="ResponseInfo">
		<xs:sequence>
			<xs:element name="ResponseDate" minOccurs="1" maxOccurs="1" type="xs:date">
				<xs:annotation>
					<xs:documentation>
						Date when the response was generated.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CmProfileId" minOccurs="1" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The id for the profile in the COS CM application.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProfileStatus" minOccurs="1" maxOccurs="1" type="common:ProfileStatus">
				<xs:annotation>
					<xs:documentation>
						Status of the profile when the response was generated.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Comments" minOccurs="0" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Optional overall comments of case.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ChallengeDetails">
		<xs:sequence>
			<xs:element name="PreviousReferenceId" minOccurs="0" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The reference Id sent for the previous request
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreviousCmProfileId" minOccurs="0" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The id for the profile in the COS CM application which is being challenged.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PreviousResponseDate" minOccurs="0" maxOccurs="1" type="xs:date">
				<xs:annotation>
					<xs:documentation>
						Date when the previous response was generated.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Comments" minOccurs="0" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Comments describing the reason for the challenge.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="Exchange">
		<xs:sequence>
			<xs:element name="Name" type="common:CmAttributeString" />
			<xs:element name="Country" type="common:CmAttributeCountry" />
			<xs:element name="TickerSymbol" type="common:CmAttributeString" />
			<xs:element name="IsApprovedExchange" type="common:CmAttributeBoolean" />
		</xs:sequence>
		<xs:attribute name="id" type="xs:int"></xs:attribute>
	</xs:complexType>
	
	<xs:complexType name="Regulator">
		<xs:sequence>
			<xs:element name="Name" type="common:CmAttributeString" />
			<xs:element name="Country" minOccurs="0" type="common:CmAttributeCountry"/>
			<xs:element name="IdentificationNumber" type="common:CmAttributeString" />
			<xs:element name="IsApprovedRegulator" minOccurs="0" type="common:CmAttributeBoolean" />
			<xs:element name="SROStatus" minOccurs="0" type="common:CmAttributeString" />
		</xs:sequence>
		<xs:attribute name="id" type="xs:int"></xs:attribute>
	</xs:complexType>
	
	<xs:complexType name="Product">
		<xs:sequence>
			<xs:element name="Product" type="common:ProductList" minOccurs="1" maxOccurs="1" />
			<xs:element name="ReferenceDocuments" minOccurs="1" maxOccurs="1" type="common:ReferenceDocuments" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="License">
