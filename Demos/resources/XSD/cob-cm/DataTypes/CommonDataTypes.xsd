	<xs:complexType name="ReferenceDocuments">
		<xs:annotation>
			<xs:documentation>The list of reference documents that were previously identified for this profile.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Document" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="MCPID" type="xs:string" minOccurs="1" />
						<xs:element name="Name" type="xs:string" minOccurs="0" />
					</xs:sequence>
					<xs:attribute name="id" type="xs:int"></xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CmAttribute" abstract="true">
		<xs:sequence>
			<xs:element name="ReferenceDocuments" minOccurs="1" maxOccurs="1" type="ReferenceDocuments">
				<xs:annotation>
					<xs:documentation>The list of reference documents supporitng the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChallengeComment" minOccurs="0" maxOccurs="1" type="xs:string">
				<xs:annotation>
					<xs:documentation>The reason for challenging this attribute.</xs:documentation>
				</xs:annotation>
			</xs:element>
 			<xs:element name="Comments" minOccurs="0" maxOccurs="unbounded" type="Comments">
				<xs:annotation>
					<xs:documentation>The comment for the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="status" type="AttributeStatus" />
	</xs:complexType>

