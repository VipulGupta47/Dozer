	</xs:simpleType>

	<xs:simpleType name="LegalForm">
		<xs:annotation>
			<xs:documentation>Before starting a business, one must decide on a legal structure for it. The legal structure of a business is fundamental to the way it operates. This legal framework determines
				who shares in the profits and losses, how tax is paid, where legal liabilities rests.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Select One" />
			<xs:enumeration value="Cooperative"></xs:enumeration>
			<xs:enumeration value="Corporation/LTD"></xs:enumeration>
			<xs:enumeration value="Estate"></xs:enumeration>
			<xs:enumeration value="General Partnership (GP)"></xs:enumeration>
			<xs:enumeration value="Hindu Undivided Family"></xs:enumeration>
			<xs:enumeration value="Incorporated Company"></xs:enumeration>
			<xs:enumeration value="Limited Company"></xs:enumeration>
			<xs:enumeration value="Limited Liability Company (LLC)"></xs:enumeration>
			<xs:enumeration value="Limited Liability Limited Partnership (LLLP)"></xs:enumeration>
			<xs:enumeration value="Limited Liability Partnership (LLP)"></xs:enumeration>
			<xs:enumeration value="Limited Partnership (LP)"></xs:enumeration>
			<xs:enumeration value="Private Limited Company"></xs:enumeration>
			<xs:enumeration value="Partnership"></xs:enumeration>
			<xs:enumeration value="Professional Corporations"></xs:enumeration>
			<xs:enumeration value="Private company limited by guarantee"></xs:enumeration>
			<xs:enumeration value="Public Limited Company"></xs:enumeration>
			<xs:enumeration value="Professional Limited Liability Company"></xs:enumeration>
			<xs:enumeration value="Public Sector Undertaking (PSU)"></xs:enumeration>
			<xs:enumeration value="Sole Proprietorship"></xs:enumeration>
			<xs:enumeration value="Trust"></xs:enumeration>
			<xs:enumeration value="Unlimited Company"></xs:enumeration>
			<xs:enumeration value="Unincorporated Organization/Association"></xs:enumeration>
			<xs:enumeration value="Society"></xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

